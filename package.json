{
  "name": "vuiet",
  "version": "0.0.6",
  "description": "Minimal wallet manager for Sui blockhain for Vue 3",
  "author": "xrexy",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "tailwind.config.js"
  ],
  "main": "./dist/vuiet.umd.js",
  "module": "./dist/vuiet.es.js",
  "typings": "./dist/vuiet.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vuiet.es.js",
      "require": "./dist/vuiet.umd.js"
    },
    "./tailwind.config.js": "./tailwind.config.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "semantic-release": "semantic-release",
    "lib:publish": "pnpm publish -r --no-git-checks --access public"
  },
  "dependencies": {
    "@mysten/sui.js": "^0.32.2",
    "@mysten/wallet-standard": "^0.5.4",
    "@vueuse/core": "^10.1.0",
    "@wallet-standard/core": "^1.0.3",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.4",
    "semantic-release": "^21.0.2",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "@mysten/sui.js": "^0.32.2",
    "vue": "^3.2.47"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xrexy/vuiet"
  }
}
